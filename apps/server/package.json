{
  "name": "@fastify_react_monorepo/server",
  "main": "src/main.ts",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint --fix --config .eslintrc.js",
    "test": "pnpm prisma:generate && jest --config jest.config.js",
    "test:cov": "pnpm prisma:generate && jest --config jest.config.js --colors --coverage",
    "test:e2e": "pnpm prisma:generate && jest --config jest.config.e2e.js",
    "lint-staged": "lint-staged --config .lintstagedrc.js",
    "db:migrate": "pnpm prisma migrate deploy",
    "prisma:generate": "pnpm prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.721.0",
    "@aws-sdk/client-s3": "^3.717.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/websocket": "^10.0.1",
    "@prisma/client": "5.13.0",
    "@slack/web-api": "^7.1.0",
    "aws-jwt-verify": "^4.0.1",
    "aws-sdk-client-mock": "^4.0.1",
    "cuid": "^3.0.0",
    "date-fns": "^4.1.0",
    "fastify": "^4.26.2",
    "json-schema-to-ts": "^3.0.1",
    "ramda": "^0.30.0",
    "ts-essentials": "^9.4.2",
    "ts-pattern": "^5.1.1"
  },
  "devDependencies": {
    "@secretlint/secretlint-rule-preset-recommend": "^8.2.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.3.1",
    "@types/ramda": "^0.29.12",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "axios-mock-adapter": "^2.0.0",
    "csv-parser": "^3.2.0",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-functional": "^6.5.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "prisma-case-format": "^2.2.1",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-mock": "^0.10.2",
    "secretlint": "^8.2.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
}